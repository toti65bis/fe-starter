{
  "name": "nextjs-starter",
  "version": "1.0.0",
  "description": "Nextjs Starter",
  "main": "server/index.js",
  "scripts": {
    "dev": "set NODE_ENV=development node server/index.js",
    "start": "nodemon -w server server/index.js",
    "serve": "set NODE_ENV=production node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudemex/nextjs-starter.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "webpack",
    "node"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix --ext .ts,.tsx,.js",
      "git add"
    ]
  },
  "files": [
    "build"
  ],
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "author": "Maximiliano \"Mex\" Delgado <mdelgado@tresdoce.com.ar>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rudemex/nextjs-starter/issues"
  },
  "homepage": "https://github.com/rudemex/nextjs-starter#readme",
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@babel/runtime": "^7.10.3",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.19.0",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.1",
    "config": "^3.0.1",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.5",
    "express": "^4.16.2",
    "helmet": "^3.20.0",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "next": "^9.3.6",
    "next-compose-plugins": "^2.2.0",
    "next-redux-wrapper": "^5.0.0",
    "next-routes": "^1.4.2",
    "prop-types": "^15.6.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-is": "^16.13.1",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.0",
    "styled-normalize": "^8.0.6",
    "unorm": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "idx": "^2.5.6",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.1",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.2.0",
    "snazzy": "^8.0.0",
    "standard": "^13.0.1",
    "typescript": "^3.9.5"
  }
}
