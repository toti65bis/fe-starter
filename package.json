{
  "name": "nextjs-starter",
  "version": "1.0.0",
  "description": "Nextjs Starter",
  "main": "server/index.js",
  "scripts": {
    "dev": "set NODE_ENV=development node server/index.js",
    "build": "set NODE_ENV=production next build",
    "start": "nodemon -w server server/index.js",
    "serve": "set NODE_ENV=production node server/index.js",
    "export": "next export -o dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudemex/nextjs-starter.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "webpack",
    "node"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "author": "Maximiliano \"Mex\" Delgado <mdelgado@tresdoce.com.ar>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rudemex/nextjs-starter/issues"
  },
  "homepage": "https://github.com/rudemex/nextjs-starter#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "cookie-parser": "^1.4.4",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "helmet": "^3.20.0",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "idx": "^2.5.6",
    "next": "^9.3.6",
    "next-redux-wrapper": "^5.0.0",
    "next-routes": "^1.4.2",
    "prop-types": "^15.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-is": "^16.13.1",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.0",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.1",
    "react-test-renderer": "^16.2.0",
    "snazzy": "^8.0.0",
    "standard": "^13.0.1"
  }
}
